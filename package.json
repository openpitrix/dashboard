  "scripts": {
    "dev": "DEBUG=koa*,-koa-views BABEL_ENV=server COMPILE_CLIENT=true NODE_ENV=development nodemon -w lib -w server --exec node server/server.js",
    "prod": "rimraf dist && npm run pack:prod && npm run serve:dist",
    "lint": "eslint . --fix",
    "test": "NODE_ENV=testing jest --config jest.config.json --updateSnapshot --coverage",
    "pack:dev": "NODE_ENV=development webpack --config webpack.dev.js --progress -w",
    "pack:prod": "NODE_ENV=production webpack -p --config webpack.prod.js --progress",
    "serve:dist": "DEBUG=koa*,-koa-views node dist/server.js",
    "precommit": "yarn test && lint-staged",
    "bundle:analyse": "webpack --config webpack.dev.js --profile --progress --json > reports/stats.json && webpack-bundle-analyzer reports/stats.json",
    "report-cov": "cat ./coverage/lcov.info | codecov"
  },
  "lint-staged": {
    "*.{js,jsx,md,test.js.snap}": [
      "prettier --write",
      "git add"
