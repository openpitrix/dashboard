  "scripts": {
    "dev:client": "NODE_ENV=development webpack --config webpack.dev.js --progress",
    "dev:serve": "DEBUG_DEPTH=5 DEBUG=app BABEL_ENV=server NODE_ENV=development nodemon -w lib -w server server/server.js",
    "dev": "npm-run-all -p dev:serve 'dev:client --watch'",
    "clean:dev": "rimraf build",
    "clean:prod": "rimraf dist",
    "prod:build": "npm run clean:prod && NODE_ENV=production webpack -p --config webpack.prod.js",
    "prod:serve": "NODE_ENV=production DEBUG=app node dist/server.js",
    "prod": "npm run prod:build && npm run prod:serve",
    "build:prod-client": "node build.js --prod-client",
    "build:prod-server": "node build.js --prod-server",
    "lint-fix": "eslint . --fix",
    "test": "NODE_ENV=test jest --config jest.config.json -u",
    "precommit": "yarn test && lint-staged",
    "viz": "webpack --config webpack.dev.js --profile --progress --json > reports/stats.json && webpack-bundle-analyzer reports/stats.json",
    "report-cov": "cat ./coverage/lcov.info | codecov",
    "dbg": "BABEL_ENV=server NODE_ENV=development node --inspect-brk server/server.js",
    "e2e": "testcafe chrome test/e2e/*.spec.js --debug-on-fail --page-load-timeout 5000",
    "commitmsg": "commitlint --config ./config/commitlint.config.js -E GIT_PARAMS",
    "release": "standard-version -i CHANGELOG.md -n"
  },
  "lint-staged": {
    "*.{js,jsx,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@commitlint/cli": "^7.0.0",
    "autoprefixer": "7.1.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.0.6",
    "babel-loader": "7.1.1",
    "babel-plugin-css-modules-transform": "^1.5.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.2",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.6",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23",
    "lint-staged": "^7.0.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "prettier": "^1.12.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "standard-version": "^4.4.0",
    "style-loader": "^0.19.1",
    "testcafe": "^0.20.4",
    "testcafe-react-selectors": "^2.1.0",
    "url-loader": "0.5.9",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": ">= 7.6.0"
  }
}
