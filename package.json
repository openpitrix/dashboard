    "semver": "^5.4.1",
    "serve-favicon": "^2.4.5",
    "winston": "^3.1.0"
  },
  "scripts": {
    "dev:client": "cross-env DEBUG=app NODE_ENV=development webpack --config webpack.dev.js --progress -w",
    "dev:serve": "cross-env DEBUG=app NODE_ENV=development rimraf build && node start-server.js",
    "dev": "cross-env run-p watch:locale dev:serve",
    "prod:build": "rimraf dist && npm run locale && NODE_ENV=production webpack -p --config webpack.prod.js",
    "prod:serve": "NODE_ENV=production node dist/server.js",
    "prod": "cross-env run-s prod:build prod:serve",
    "lint-fix": "eslint --fix --ext js,jsx .",
    "test": "jest -u",
    "e2e:client": "cross-env DEBUG=app NODE_ENV=development webpack --config webpack.dev.js --define process.env.MOCKAPI=1",
    "e2e:serve": "cross-env MOCKAPI=1 DEBUG=app NODE_ENV=development node server/server.js",
    "e2e:env": "cross-env MOCKAPI=1 run-s locale e2e:client e2e:serve",
    "e2e:record": "start-test e2e:env 8000 cy:record",
    "e2e:open": "start-test e2e:env 8000 cy:open",
    "e2e:run": "start-test e2e:env 8000 cy:run",
    "e2e:run:local": "start-test e2e:env 8000 'cypress run'",
    "cy:open": "cypress open",
    "cy:record": "cypress open --env recordApi=1",
    "cy:run": "cypress run --record --key 2a1d655c-7a4d-4a3a-8a31-0683b816f7e6",
    "cy:verify": "cypress verify",
    "precommit": "lint-staged",
    "report-cov": "cat ./build/coverage/lcov.info | codecov",
    "commitmsg": "commitlint --config ./config/commitlint.config.js -E GIT_PARAMS",
    "changelog": "changelog -u https://github.com/openpitrix/dashboard",
    "locale": "./src/bin/locale.js merge all",
    "watch:locale": "cross-env npm run locale -- --watch",
    "dbg": "cross-env NODE_ENV=development node --inspect-brk server/server.js"
  },
  "lint-staged": {
    "*.{js,jsx,md,scss,css}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "bail": true,
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(png|svg)$": "<rootDir>/config/assets.transform.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "coverageDirectory": "./build/coverage/",
    "collectCoverageFrom": [
      "src/components/**/*.jsx"
    ],
    "testPathIgnorePatterns": [
      "/cypress/",
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@commitlint/cli": "^7.0.0",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "autoprefixer": "7.1.2",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "cache-loader": "^3.0.0",
    "codecov": "^3.5.0",
    "commander": "^2.20.0",
    "compression-webpack-plugin": "^1.1.11",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "cypress": "^3.3.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^1.1.6",
    "generate-changelog": "^1.7.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "koa-webpack": "^5.2.4",
    "lint-staged": "^7.0.4",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "prettier": "^1.17.1",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "standard-version": "^4.4.0",
    "start-server-and-test": "^1.9.1",
