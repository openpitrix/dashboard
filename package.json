    "chokidar": "^2.0.4",
    "classnames": "^2.2.5",
    "clipboard": "^2.0.1",
    "dayjs": "^1.7.4",
    "debug": "^3.1.0",
    "file-saver": "^2.0.0",
    "history": "^4.7.2",
    "http-proxy": "^1.17.0",
    "i18next": "^15.0.7",
    "i18next-browser-languagedetector": "^2.2.3",
    "js-base64": "^2.4.5",
    "js-yaml": "^3.10.0",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-favicon": "^2.0.0",
    "koa-helmet": "^4.0.0",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.3.0",
    "koa-session": "^5.8.1",
    "koa-static": "^4.0.2",
    "lodash": "^4.17.5",
    "mitt": "^1.1.3",
    "mobx": "^5.1.0",
    "mobx-react": "^5.3.6",
    "path-to-regexp": "^3.0.0",
    "promise-polyfill": "^7.0.0",
    "prop-types": "^15.6.0",
    "rc-table": "^6.1.2",
    "rc-tree": "^1.14.8",
    "react": "^16.6.0",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.6.0",
    "react-i18next": "^10.5.1",
    "react-markdown": "^3.3.4",
    "react-modal": "^3.3.1",
    "react-popper": "^0.10.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.6.1",
    "semver": "^5.4.1",
    "serve-favicon": "^2.4.5",
    "superagent": "^3.8.3",
    "winston": "^3.1.0"
  },
  "scripts": {
    "dev:client": "cross-env DEBUG=app NODE_ENV=development webpack --config webpack.dev.js --progress",
    "dev:serve": "cross-env DEBUG_DEPTH=5 DEBUG=app BABEL_ENV=server NODE_ENV=development nodemon -w lib -w server server/server.js",
    "dev": "cross-env npm-run-all -p watch:locale dev:serve 'dev:client -- --watch'",
    "prod:build": "rimraf dist && npm run locale && NODE_ENV=production webpack -p --config webpack.prod.js --progress",
    "prod:serve": "NODE_ENV=production node dist/server.js",
    "prod": "npm run prod:build && npm run prod:serve",
    "lint-fix": "eslint --fix --ext js,jsx .",
    "test": "jest -u",
    "precommit": "lint-staged",
    "viz:bundle": "webpack --config webpack.dev.js --profile --json > build/stats.json && webpack-bundle-analyzer build/stats.json",
    "report-cov": "cat ./build/coverage/lcov.info | codecov",
    "commitmsg": "commitlint --config ./config/commitlint.config.js -E GIT_PARAMS",
    "release": "standard-version -i CHANGELOG.md -n",
    "locale": "DEBUG=locale ./src/bin/locale.js merge all",
    "watch:locale": "cross-env npm run locale -- --watch"
  },
  "lint-staged": {
    "*.{js,jsx,md,scss,css}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "bail": true,
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(png|svg)$": "<rootDir>/config/assets.transform.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "coverageDirectory": "./build/coverage/",
    "collectCoverageFrom": [
      "src/components/**/*.jsx"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^7.0.0",
    "autoprefixer": "7.1.2",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-css-modules-transform": "^1.5.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "cache-loader": "^3.0.0",
    "codecov": "^3.0.2",
    "commander": "^2.19.0",
    "compression-webpack-plugin": "^1.1.11",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^1.1.6",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^7.0.4",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "prettier": "^1.12.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "standard-version": "^4.4.0",
    "style-loader": "^0.19.1",
    "url-loader": "^1.1.2",
    "webpack": "4.29.5",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "3.2.3",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.7.0"
  },
  "engines": {
    "node": ">= 7.6.0"
  }
}
