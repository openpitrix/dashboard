    "react-codemirror": "^1.0.0",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.11.0",
    "react-markdown": "^3.3.4",
    "react-modal": "^3.3.1",
    "react-popper": "^0.10.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
