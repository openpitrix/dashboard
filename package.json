    "koa-session": "^5.8.1",
    "koa-static": "^4.0.2",
    "lodash": "^4.17.5",
    "mobx": "^5.1.0",
    "mobx-react": "^5.3.6",
    "promise-polyfill": "^7.0.0",
    "prop-types": "^15.6.0",
    "rc-table": "^6.1.2",
    "react": "^16.6.0",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.6.0",
    "react-i18next": "^7.7.0",
    "react-markdown": "^3.3.4",
    "react-modal": "^3.3.1",
    "react-popper": ">=0.10",
