{
  "name": "openpitrix-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "app-root-path": "^2.0.1",
    "babel-runtime": "^6.26.0",
    "chokidar": "^2.0.4",
    "classnames": "^2.2.5",
    "debug": "^3.1.0",
    "deepmerge": "^2.1.0",
    "js-base64": "^2.4.5",
    "js-yaml": "^3.10.0",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-favicon": "^2.0.0",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.3.0",
    "koa-session": "^5.8.1",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.3",
    "lodash": "^4.17.5",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "promise-polyfill": "^7.0.0",
    "prop-types": "^15.6.0",
    "pug": "^2.0.0-rc.4",
    "rc-pagination": "^1.14.0",
    "rc-table": "^6.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-markdown": "^3.3.4",
    "react-modal": "^3.3.1",
    "react-popper": ">=0.10",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^5.0.0-alpha.4",
    "semver": "^5.4.1",
    "serve-favicon": "^2.4.5",
    "superagent": "^3.8.3"
  },
  "scripts": {
    "dev:client": "NODE_ENV=development webpack --config webpack.dev.js --progress",
    "dev:serve": "DEBUG=op-dash,koa-*,-koa-views,-koa-router BABEL_ENV=server COMPILE_CLIENT=true NODE_ENV=development node server/server.js",
    "server": "DEBUG=op-dash BABEL_ENV=server NODE_ENV=development node server/server.js",
    "dev:watch": "npm run dev:client -- --watch",
    "dev": "rimraf build && npm run dev:client && npm run server",
    "prod:build": "NODE_ENV=production webpack -p --config webpack.prod.js --progress",
    "prod:serve": "NODE_ENV=production DEBUG=op-dash,koa-*,-koa-router,-koa-views node dist/server.js",
    "prod": "rimraf dist && npm run prod:build && npm run prod:serve",
    "lint": "eslint . --fix",
    "test:single": "NODE_ENV=testing jest --config jest.config.json -u",
    "test": "npm run test:single -- --coverage",
    "precommit": "yarn test && lint-staged",
    "viz": "webpack --config webpack.dev.js --profile --progress --json > reports/stats.json && webpack-bundle-analyzer reports/stats.json",
    "report-cov": "cat ./coverage/lcov.info | codecov",
    "dbg": "BABEL_ENV=server NODE_ENV=development node --inspect-brk server/server.js"
  },
  "lint-staged": {
    "*.{js,jsx,md,test.js.snap}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.0.6",
    "babel-loader": "7.1.1",
    "babel-plugin-css-modules-transform": "^1.5.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "1.1.3",
    "codecov": "^3.0.2",
    "css-loader": "0.28.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "^1.1.6",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23",
    "koa-webpack": "^2.0.3",
    "lint-staged": "^7.0.4",
    "node-sass": "^4.5.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "prettier": "^1.12.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "url-loader": "0.5.9",
    "webpack": "^3",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": ">= 7.6.0"
  }
}
